#HTTP Server
server:
  port: 3333
  
spring:
  jpa:
    properties:
      hibernate:
        jdbc: time_zone = TimeZone

#Discovery Server Access
eureka:
  client:
    service-url:
      default-zone: http://localhost:8761/eureka
  instance:
    status-page-url-path: /infoCarrito
    health-check-url-path: /healthCarrito
