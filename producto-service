#HTTP Server
server:
  port: 2222
  
spring:
  jpa:
    properties:
      hibernate:
        jdbc: time_zone = TimeZone


#Discovery Server Access
eureka:
  client:
    service-url:
      default-zone: http://localhost:8761/eureka
  instance:
    status-page-url-path: /infoProducto
    health-check-url-path: /healthProducto
